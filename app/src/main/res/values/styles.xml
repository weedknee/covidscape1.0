<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="roundImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">20dp</item>
        <item name="strokeWidth">10dp</item>
        <item name="strokeColor">@color/black</item>
        <item name="color">@color/black</item>
    </style>

    <!--transparent pop up-->
    <style name="Theme.AppCompat.transparent_bg" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>

    <style name="yelllowbg" parent="">
        <item name="color">@color/yellow</item>
    </style>
</resources>